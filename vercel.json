{
  "version": 2,
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/frontend/:path*",
      "destination": "/frontend/:path*"
    },
    {
      "source": "/calculator/:path*",
      "destination": "/calculator/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/index.html"
    }
  ]
}

